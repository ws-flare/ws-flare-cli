{
  "name": "ws-flare-cli",
  "version": "1.0.0",
  "description": "ws-flare-cli",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "nyc --reporter=text-summary --reporter=html lb-mocha --allow-console-logs \"dist/__tests__\"",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.2.3",
    "@loopback/context": "^1.12.0",
    "@loopback/core": "^1.6.0",
    "@loopback/openapi-v3": "^1.3.7",
    "@loopback/repository": "^1.5.1",
    "@loopback/rest": "^1.10.4",
    "@loopback/rest-explorer": "^1.1.18",
    "@loopback/service-proxy": "^1.1.6",
    "winston": "^3.2.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.5.0",
    "@loopback/testlab": "^1.2.5",
    "@loopback/tslint-config": "^2.0.4",
    "@types/node": "^10.11.2",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^13.0.0",
    "coveralls": "^3.0.3",
    "nyc": "^14.1.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  }
}
